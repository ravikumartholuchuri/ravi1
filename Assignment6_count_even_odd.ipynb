    "list1 = [10, 21, 4, 45, 66, 93, 54, 77, 1] \n",
    "even_count, odd_count = 0, 0\n",
    "for num in list1: \n",
    "    if num % 2 == 0: \n",
    "        even_count += 1\n",
    "    else: \n",
    "        odd_count += 1\n",
    "print(\"Even numbers in the list: \", even_count) \n",
    "print(\"Odd numbers in the list: \", odd_count) "
